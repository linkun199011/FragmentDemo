<?xml version="1.0" encoding="utf-8"?>
<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">


        <Button
            android:id="@+id/registerAppCallback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册应用监听"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/unregisterAppCallback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="反注册应用监听"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/testForOthers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置返回键(测试用)"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/navigation_bar_divider_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="导航栏+divider改色"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/navigation_bar_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="仅导航栏改色"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/js_cache_dao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Js注入CacheDao"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/js_new_instance_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Js创建新实例" />

        <Button
            android:id="@+id/local_js_unit_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="js单元测试（本地）" />

        <Button
            android:id="@+id/autoTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="自动化测试js接口(本地调试)" />

        <Button
            android:id="@+id/btn_test_file_sdcard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文件管理(sdcard)" />

        <Button
            android:id="@+id/btn_test_file_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文件管理(Asset)" />

        <Button
            android:id="@+id/btn_test_file_http"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文件管理(http/https)" />

        <Button
            android:id="@+id/btn_test_callback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="callback测试" />

        <EditText
            android:id="@+id/etUrl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <Button
            android:id="@+id/btn_test_download_http"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="进入测试地址页面" />

        <Button
            android:id="@+id/btn_test_download"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试内置下载页面" />

        <Button
            android:id="@+id/btn_test_gps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="获取地理位置信息" />

        <Button
            android:id="@+id/btn_local_storage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="数据本地存储测试" />

        <Button
            android:id="@+id/restDao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="restDao测试" />

        <Button
            android:id="@+id/key_menu_name_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="添加key_menu_name_id" />

        <Button
            android:id="@+id/register_menu1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册和反注册menu(有更多按钮)" />

        <Button
            android:id="@+id/register_menu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册和反注册menu(无更多按钮)" />

        <Button
            android:id="@+id/maf_btn_ids"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="水平菜单(url传id)" />

        <Button
            android:id="@+id/register_menu3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="test" />

        <Button
            android:id="@+id/visit_baidu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="访问百度" />


        <Button
            android:id="@+id/scroll_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="滚动条自定义" />

        <Button
            android:id="@+id/webview_core"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="webview内核" />

        <EditText
            android:id="@+id/address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   https://www.baidu.com/" />

        <Button
            android:id="@+id/visit_custom_url_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="访问以上网址(close)" />

        <Button
            android:id="@+id/visit_custom_url_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="访问以上网址(back)" />

        <Button
            android:id="@+id/visit_custom_url_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="访问以上网址(none)" />

        <Button
            android:id="@+id/tv_test_pulldown_before"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试下拉刷新(以前)" />

        <Button
            android:id="@+id/tv_test_pulldown_after"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试下拉刷新(现在)" />

        <EditText
            android:id="@+id/addCacheOpen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   https://www.baidu.com/?_maf_android_temp_cache_open=true" />

        <Button
            android:id="@+id/visit_cache_open"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="访问开启缓存的百度" />

        <Button
            android:id="@+id/testForDownload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="进入IM下载测试页面promise"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/testForDownload2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="进入IM下载测试页面非promise" />

        <Button
            android:id="@+id/btn_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分享" />

        <Button
            android:id="@+id/btn_read_h5_file"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="读取离线轻应用文件内容" />


        <Button
            android:id="@+id/btn_test_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试图片加载" />

        <Button
            android:id="@+id/btn_test_hash"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="测试网页中打开应用" />


        <EditText
            android:id="@+id/et_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1" />

        <EditText
            android:id="@+id/et_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="2" />

        <EditText
            android:id="@+id/et_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="3" />

        <EditText
            android:id="@+id/et_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4" />

        <EditText
            android:id="@+id/et_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="5" />

        <EditText
            android:id="@+id/et_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="6" />

        <EditText
            android:id="@+id/et_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="7" />

        <EditText
            android:id="@+id/et_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="8" />

        <EditText
            android:id="@+id/et_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="9" />

        <TextView
            android:id="@+id/phone_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />

    </LinearLayout>
</HorizontalScrollView>